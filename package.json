{
  "name": "ng-swiss-army-knife",
  "version": "1.3.28",
  "scripts": {
    "postversion": "git push --follow-tags",
    "compodoc": "compodoc --config .compodocrc.json",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "testCI": "ng test --karma-config=projects/ng-swiss-army-knife/karma.conf.ci.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "copyFiles": "copyfiles -u -1 ./projects/ng-swiss-army-knife/src/lib/styling/atoms.scss ./dist/ng-swiss-army-knife",
    "build:lib:scss": "node-sass ./projects/ng-swiss-army-knife/src/lib/styling/atoms.scss ./dist/ng-swiss-army-knife/atoms.css",
    "build-lib": "ng build ng-swiss-army-knife && npm run build:lib:scss && npm run copyFiles",
    "npm-pack": "cd dist/ng-swiss-army-knife && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish": "cd dist/ng-swiss-army-knife && npm publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.1.4",
    "@angular/common": "~9.1.4",
    "@angular/compiler": "~9.1.4",
    "@angular/core": "~9.1.4",
    "@angular/forms": "~9.1.4",
    "@angular/platform-browser": "~9.1.4",
    "@angular/platform-browser-dynamic": "~9.1.4",
    "@angular/router": "~9.1.4",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.4",
    "rxjs-compat": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.4",
    "@angular-devkit/build-ng-packagr": "^0.901.4",
    "@angular/cli": "~9.1.4",
    "@angular/compiler-cli": "~9.1.4",
    "@angular/language-service": "~9.1.4",
    "@compodoc/compodoc": "^1.1.7",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "~4.5.0",
    "copyfiles": "^2.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ng-packagr": "^9.1.3",
    "node-sass": "^4.14.0",
    "scss-bundle": "^2.5.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.8.3"
  }
}
